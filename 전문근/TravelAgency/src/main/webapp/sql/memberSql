create table member(
memId varchar2(30) primary key,
memPwd varchar2(30) not null,
memName varchar2(30) not null,
memBirth01 varchar2(5) not null,
memBirth02 varchar2(5) not null,
memBirth03 varchar2(5) not null,
memGender varchar2(3) not null,
memEmail varchar2(20) not null,
memEmail0 varchar2(3),
memZipCode varchar2(50) not null,
memAddr1 varchar2(70) not null,
memAddr2 varchar2(50) not null,
memMobile01 varchar2(10) not null,
memMobile02 varchar2(10) not null,
memMobile03 varchar2(10) not null,
contry varchar2(20),
logtime date,
lastName varchar2(6) default null,
firstName varchar2(20) default null,
passportNumber varchar2(10) default null,
passportStartYear varchar2(6) default null,
passportStartMonth varchar2(6) default null,
passportStartDay varchar2(6) default null,
passportEndYear varchar2(6) default null,
passportEndMonth varchar2(6) default null,
passportEndDay varchar2(6) default null,
visaContry varchar2(6) default null,
visaStartYear varchar2(6) default null,
visaStartMonth varchar2(6) default null,
visaStartDay varchar2(6) default null,
visaEndYear varchar2(6) default null,
visaEndMonth varchar2(6) default null,
visaEndDay varchar2(6) default null
);

create table newzipcode (
zipcode varchar2(50),
sido varchar2(50),
sigungu varchar2(50),
yubmyundong varchar2(50),
ri varchar2(50),
roadname varchar2(100),
buildingname varchar2(100)
);